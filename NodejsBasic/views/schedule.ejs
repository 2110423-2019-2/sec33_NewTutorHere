        <div class="cd-schedule__events">
            <ul>
                <li class="cd-schedule__group">
                    <div class="cd-schedule__top-info"><span>Monday</span></div>
                    <ul>

                        <% schedule[0].forEach(element => { %>

                        <li class="cd-schedule__event">
                            <a data-start="<%= element.class_start %>" data-end="<%= element.class_end %>"
                                data-content="<%= element.course_name %>" data-event="event-1" href="#0">
                                <em class="cd-schedule__name"><%= element.course_name %></em>
                            </a>
                        </li>
                        <% }) %>

                    </ul>
                </li>

                <li class="cd-schedule__group">
                    <div class="cd-schedule__top-info"><span>Tuesday</span></div>

<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>NewTutorHere</title>
  <link rel="stylesheet" type="text/css" href="/stylesheets/style.css" />
  <!-- <link rel="stylesheet" type="text/css" href="css/index.css" /> -->
  <!-- <link rel="shortcut icon" href="img/dog.ico" /> -->

  <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script> -->
  <!-- <script src="https://unpkg.com/axios/dist/axios.min.js"></script> -->
  <!-- <script src="script.js"></script> -->
  <!-- <script>checkStatus()</script> -->
  <script type="text/javascript" src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
  <script type="text/javascript" src="/scripts/mustache.js"></script>
  <script type="text/javascript" src="/scripts/modal.js"></script>
  <script type="text/javascript" src="/scripts/custom.js"></script>
</head>

<body>
  <%- include navbar %>
  <div class="big-bg">
    <!-- Content -->
    
      <div class="contractlog-box be-left">
        <span class="name-stucontract">
          <p>Contract Log</p>
        </span>
        <div class="conlog-item">
          <span class="name-stucontract">
            <a>Student One</a>
          </span>
          <br />
          <button class="view2-button search-button be-right">Terminate</button>
          <button class="view2-button search-button be-right">View</button>
        </div>

        <div class="conlog-item">
          <span class="name-stucontract">
            <a>Student Two</a>
          </span>
          <br />
          <button class="view2-button search-button be-right">Terminate</button>
          <button class="view2-button search-button be-right">View</button>
        </div>
      </div>
      <div class="contract-box color-lightgreen be-right" id="contract-form">
        <!-- Begin of contract list -->

        <% tutor_contract.forEach(element => { %>

          <form class="form" id="contract">
            <img class="nav-img-user" src="../img/user.png" style="height: 5em;" />
            <div class="c-box be-right">
                <div class="blockcontract">
                    <span class="name-contract">
                        <p><%= element.student_username %> </p>
                        <a>sends you a contract.</a>
                    </span>
                    <br />
                    <button class="view-button search-button be-right" >View a contract</button>
                    <button class="view-button search-button be-right" >View profile</button>
                </div>
            </div>
  
          </form>
         
        <% }) %>
        
      </div>

    
  </div>

  <!-- <script>
    generateContract();
  </script> -->

</body>

</html>